    @include placeholder(){ color: $_color_newsletter; font-size: $_font_size; font-weight: $font_weight_normal; }    

  }
  /* End Popup Newsletter */
  /* up down page*/
  .float-right-icon{ $_width: 50px; overflow: hidden;
    position: fixed; right: 20px; bottom: 75px; padding: 5px;       
    ul{ list-style: none;}  
    #scroll-to-top{ position: relative; @include transition(); margin-left: auto; padding: 0; text-align: center;
      background-color: $color_primary;
      @extend .btn; @include btn-width($_width, 1px); width: $_width; border-radius: 50%;   
      color: #fff;
      &:after{ content: ''; position: absolute; top: 4px; left: 4px; right: 0;
        bottom: 0;
        margin-left: -1px; margin-bottom: -1px; border-radius: 50%;
        @include box-shadow(1px 1px 0 #fff);
      }
      &:hover{ cursor: pointer;        
        &:after{ @include animation(spinAround 2s linear infinite); }
      }
      &.off{ margin-right: -100px; }
    }
    @media(max-width: 767px){ display: none; }
  }
  /* end up down page*/
  /*Ajax cart  */
  .popup-pro-wrapper {
    z-index: 999;      
    @include display-flexbox();    
    @include align-items(center);  
    .ajax-cart-overlay {      
      @include style_overlay();
      position: fixed; top: 0; left: 0; right: 0; bottom: 0;      
    }
    .ajax-cart-modal-wrapper {      
      margin: $distance_section_subpage auto;          
      width: $_width_popupwrapper + 2*15px;  
      padding-left: 15px;
      padding-right: 15px;
      .ajax-cart-box {         
        position: relative;
        background-color: $color_content_bg;                       
        .ajax-cart-close {
          $font_size_cart_close: 15px;
          $width_cart_close: 30px;
          margin-top: 10px;
          margin-right: 10px;
          position: absolute;
          top: 0;
          left: auto;
          right: 0;
          width: $width_cart_close;
          height: $width_cart_close;
          text-align: center;                    
          cursor: pointer;
          padding: 0;    
          .cs-icon {             
            font-size:  $font_size_cart_close;
            color: #fff;            
            line-height: $width_cart_close;
            width: 100%;
            text-align: center;              
            @include transition();
          }
          &:hover, &:focus{
            .cs-icon {             
              color: $color_heading;
            }
          }
        }
        .group_ajax{
          @include display-flexbox();
          @include flex-wrap(wrap);     
          padding-top: 30px;
          padding-bottom: 30px;
          $_width_ajax-cart-left: 42%;
          .ajax-cart-left{ width: $_width_ajax-cart-left; }
          .ajax-cart-right{ width: $width_full - $_width_ajax-cart-left; }           
          .ajax-cart-left{ 
            padding-left: 30px;
            padding-right: 30px; 
            border-right: 1px solid $color_line;
            .ajax-cart-product{
              @include clearfix;
              > div{ padding: 0; }                                         
              .ajax-cart-image{
                padding-right: 25px;
                > a{
                  display: block;                   
                }
              }
              .ajax-cart-desc{
                $___margin_bottom: 15px;
                .ajax-cart-product-title{
                  line-height: 2em;
                  margin-bottom: $___margin_bottom;
                  a{
                    font-size: $_h6;
                    color: $color_product_title_text;
                    font-weight: $font-weight_bold;
                    &:hover{                      
                      color: $color_product_title_text_hover;
                    }
                  }    
                }                
                .ajax-cart-price{                  
                  margin-bottom: $___margin_bottom;
                  font-weight: $font-weight_bold;
                  .money{
                    font-size: $_h6;
                    color: $color_price_text;
                  }
                }
                .ajax-cart-variant,
                .ajax-cart-qty{
                  font-size: $_h7;
                  color: $color_tertiary;
                  margin-bottom: 5px;
                }
              }
            }
          }
          .ajax-cart-right {   
            padding-left: 30px;
            padding-right: 30px;
            .ajax-cart-number-product {              
              margin-bottom: 15px;
              font-size: $_h6;
              text-transform: uppercase;
              font-weight: $font_weight_normal;
              color: $color_heading;
            }
            .ajax-cart-subtotal{
              span {
                font-weight: $font_weight_normal;
                font-size: $_h3;
                color: $color_heading;
              }
            }            
            .ajax-cart-group-actions { 
              @include display-flexbox();
              @include flex-wrap(nowrap);
              @include align-items(center);               
              margin-top: 50px;
              ._btn{
                @include btn_large;                 
              }
              .ajax-cart-continue,
              .ajax-cart-next{
                ._btn{ 
                  @include  btn_pri;
                  padding: 0 30px;
                  &:focus, &:hover{ 
                    @include btn1; 
                  }
                }
              }              
              .ajax-cart-next{
                margin-right: 10px;
              }              
            }
            .countDiv {
              font-size: $_h7;
              font-style: italic;
              display: block;
              width: 100%;
              float: left;
              margin-top: 22px;
              span {                            
                color: $color_sub;
              }
            }
          }   
        }          
      }      
    }
    .ajax-cart-note{
      @extend ._alert;
      @extend .aler_addtocart;

      max-width: 100%;
      margin-bottom: 0;
      padding-right: 50px;
    }    
    @include respond-to($media_max_tabletsmall){
      .ajax-cart-modal-wrapper .ajax-cart-box .group_ajax{
        @include flex-wrap(nowrap); 
        .ajax-cart-left{
          width: 100%;
          max-width: 250px;
          .ajax-cart-product .ajax-cart-image{
            padding-right: 0;
            margin-bottom: 15px;
          }
        }        
        .ajax-cart-right{
          width: 100%;
        }
      }
    }    
  }
  /*End Ajax cart*/
  /*wishlist_compare*/  
  @mixin model_wishlist_compare{
    .modal{
      @include _lightbox(700px);
      .max_compare{
        padding-top: 30px;
        margin-bottom: 0;
      }      
    }    
    .modal-body{    
      $_width_compare_left: 30%;
      padding: 0 30px 30px 30px !important;
      .modal-note{                
        @extend  ._alert;
        @extend .aler_addtocart;        
        margin-bottom: 30px;
        margin-left: -30px;
        margin-right: -30px;        
        max-width: inherit;
        width: auto;
        text-align: left !important;
      }                      
      .product-right{
        margin-bottom: 20px;
        text-align: left;
        .name{
          font-size: $_h6;
        }
        .price{
          color: $color_price_text;
          font-size: $_h4;
        }
        .price_compare{
          color: $color_price_compare_text;
          font-size: $_h6;
          margin-left: 10px;
        }
      }
      form{
        ._btn{
          @include btn_medium;
          width: 100%;
          margin-bottom: 10px;
          @include btn_pri;
          &:focus,
          &:hover{
            @include btn1;
          }
        }
        .selector-wrapper,
        .quantity-content{  
          text-align: left;
          @include input_tb;
          input:not(._btn),               
          select:not(._btn){
            height: 40px;
            line-height: 40px - 4px;
            padding-left: 10px;
            padding-right: 10px;
            margin-bottom: 10px;
          }              
        }
        .quantity-content{     
          label{
            margin-right: 2px;
          }
        }            
      }
      .modal-body-inner{
        @include display-flexbox();
        @include flex-wrap(nowrap);      
        .modal-left{
          width: $_width_compare_left;                   
          padding-right: 30px;          
        }
        .modal-right{
          width: $width_full - $_width_compare_left;                              
          form{
            @include display-flexbox();
            @include flex-wrap(wrap);      
            > div{
              width: 50%;
            }              
            .form-right{
              border-left: 1px solid $color_line;
              padding-left: 30px;
            }
          }           
        }
      }     
    }
    @include respond-to($media_max_mobile){      
      .modal-dialog{        
        max-width: 300px !important;              
      }            
      .modal-body{
        .product-right,
        .product-left{
          width: 100%;
        }
        .product-left{
          padding-right: 0px;
          margin-bottom: 15px;              
        } 
        .modal-body-inner {
          @include flex-wrap(wrap);    
          .modal-left{
            padding-right: 0;
          }
          .modal-right,
          .modal-left{
            width: 100%;
          }          
          .modal-right{
            margin-top: 15px;            
          }
          .modal-right{            
            form {
              > div{
                width: 100%;
              }
              .form-right{
                border-left-width: 0;
                padding-left: 0;
                margin-top: 20px;
              }
            }
          }          
        }
      }                 
    }    
  }
  .fb-overlay{
    .fb-skin{
      border-radius:0;
    }
    .fb-close,
    .fb-next span,
    .fb-prev span{
      background-image: none;                
    }
    .fb-next span,
    .fb-prev span{
      @include button-nextprev();
