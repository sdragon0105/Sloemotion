    .questions-layout{
      margin-bottom: $distance_section_subpage*2;        
    }      
    .contact-layout{        
      .contact-content{
        @include clearfix;
        @include padding_container(15px);
        margin-top: -15px;
        .feedback{
          padding-left: 15px;
          padding-right: 15px;
        }
        @include respond-to($media_max_mobile){
