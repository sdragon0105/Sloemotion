    border-width: 0;
  }
  @include respond-to($media_max_mobile){
    background-position: 5px center;
    font-size: $_h7 - 2px;        
    background-size: 60px;
    padding-left: 70px;
    padding-top: 10px;
    padding-bottom: 10px;
  } 
}
/* end alert box */
/* pagination_group */
.pagination_group{  
  padding-left: 0px;
  padding-right: 0px; 
  margin: ($distance_section_subpage * 2) auto $distance_section_subpage;
  width: 100%;
  .pagination{ 
    @include display-flexbox();
    @include flex-wrap(wrap);      
    @include justify-content(center);
    border-radius: 0;
    -webkit-margin-before: 0em;
    -webkit-margin-after: 0em;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
    -webkit-padding-start: 0px;
    li{
      display: block;         
      margin-left: 3px;
      margin-right: 3px;
      &:last-child{
        margin-right: 0;
      }
      &:first-child{
        margin-left: 0 !important;
      }
      a{           
        border-style: solid;
        border-width: 1px;
        border-color: $color_pagination_border;
        background-color: $color_pagination_bg;
        color: $color_pagination_text;
        height: 30px;
        line-height: 30px;                   
        text-transform: uppercase;
        font-size: 14px;        
        font-weight: $font_weight_normal;
        padding: 0 12px 0 12px;       
      }        
      &.prev,
      &.next{
        position: relative;
        a{
          border-radius: 0 !important;          
          padding-left: 8px;
          padding-right: 8px;            
          .cs-icon{            
            z-index: 1;
            color: $color_pagination_text;               
          }                     
          &.disabled{
            @include opacity(0.5, 50);
            cursor: default;                        
          }
        }

        &:hover, &:focus{                
          a{
            border-color: $color_pagination_border_hover;
            background-color: $color_pagination_bg_hover;
            color: $color_pagination_text_hover;                                   
            .cs-icon{
              color: $color_pagination_text_hover;
            }
          }         
        }        
      }  
      &:not(.prev){
        &:not(.next){
          &:hover, &:focus{
            a{              
              background-color: $color_pagination_bg_hover;
              color: $color_pagination_text_hover;
              border-color: $color_pagination_border_hover;
            }            
          }
        }
      }                 
      &.active{
        a{
          background-color: $color_pagination_bg_hover;
          color: $color_pagination_text_hover;
          border-color: $color_pagination_border_hover;
          .cs-icon{
            color: $color_pagination_text_hover;
          }
        }        
      }     
    }
  }
  .pagination>.active>a, 
  .pagination>.active>a:focus, 
  .pagination>.active>a:hover, 
  .pagination>.active>span, .pagination>.active>span:focus, 
  .pagination>.active>span:hover{
    border-color: $color_pagination_border;
  }
}
/* End pagination_group  */
