  /*WISHLIST & SHOPING CART PAGE*/
  /* style cart */
  $_width_image: 110px;    
  $_padding_td: 30px;
  $_border_table: 1px;
  @mixin product_name_tb{
    font-size: $_h7;
    color: $color_product_title_text;                  
    > a{ 
      color: $color_body;
      font-weight: $font_weight_normal;
      @include inline-block();
      margin-bottom: 15px;
    }
  }
  @mixin _heading_style_tb{
    font-size: $_h6;
    color: $color_tertiary;    
    text-transform: uppercase;
    text-align: center;
    vertical-align: middle;
    font-weight: $font_weight_bold1;
    padding-left: $_padding_td;
    padding-right: $_padding_td;
    padding-top: 13px;
    padding-bottom: 13px;     
    &.wishlist-image,
    &.image{
      text-align: left;
    }
  }
  @mixin product_variant_tb{
    > p{
      color: $color_heading;
      margin-bottom: 5px;
      .variant-title{
        color: $color_tertiary;
      }
    }
  }    
  @mixin image_style{ 
    width: $_width_image;
    padding-left: 0 !important;
    padding-right: 0 !important;
    img{
      max-width: 100%;
    }    
  }
  @mixin thead_tb{
    background-color: $color_content_bg;
    border-bottom: 1px solid $color_line;
  }
  @mixin _tr{
    border-style: solid;
    border-color: $color_line;
    border-width: $_border_table;           
    border-width: 0;
    border-bottom-width: 1px;
  }
  @mixin _td{
    text-align: center;
    vertical-align: middle;
    padding: $_padding_td;  
    @include respond-to($media_max_tabletsmall){
      padding: 15px;
    }
  }
  @mixin money_tb{
    .price_compare{
      font-size: $_h7;        
      color: $color_price_compare_text;
      padding-left: 10px;
      .money{  
        color: $color_price_compare_text;
        font-weight: $font_weight_normal;  
      }
    }
    .money{  
      color: $color_price_text;
      font-weight: $font_weight_bold;
    }         
  }

  section.cart-content{      
    @mixin style_input($__height: 40px){
      .form-action,
      .form-item{
        padding: 10px 0;       
      }      
      select,
      input{
        &:not(._btn){
          height: $__height;
          line-height: $__height - 2px;
          padding: 0 15px;
        }
      }
      textarea{
        height: 200px;
        padding: 20px 15px;
      }
      textarea,
      select,
      input{    
        &:not(._btn){
          width: 100%;
          background-color: $color_content_bg;
        }
      }
    }    
    margin-top: $distance_section_subpage;
    margin-bottom: $distance_section_subpage;     
    table{
      margin-bottom: $_padding_td;
      @include money_tb;
      .product_name{
        @include product_name_tb;
      }
      .product_variant{
        @include product_variant_tb;
      }        
      .quantity-wrapper{           
        @include input_tb;          
      }       
      .product_image{
        max-width: 100%; 
        > a{
          display: block;
        }
      }
      thead{
        @include thead_tb;
        tr{
          th{
            @include _heading_style_tb;              
            &:first-child{
              padding-left: 0;
            }
          }
        }
      }
      tbody{
        tr{
          @include _tr;
          td{
            @include _td;                                     
            &.image{
              @include image_style;                            
            }                    
            &.image-info{
              text-align: left;                            
              min-width: 100px;
            }   
            &.remove{
              .remove-title{
                text-transform: uppercase;                
              }
              .cs-icon{
                margin-right: 5px;
                &:before{
                  font-weight: $font_weight_bold;
                  font-size: 12px;
                }
              }
            }
          }          
          &.summary{
            color: $color_heading;
            .total_label{
              text-align: left;
              font-size: $_h6;
              text-transform: uppercase;
              font-weight: $font_weight_bold1;
              padding-left: 0;
            }
            .price{
              text-align: right;
              padding-right: 0;
              .money{
                font-size: $_h4;
                color: $color_heading;
                font-weight: $font_weight_bold1;
              }
            }
          }
        }
      }
    } 
    ._btn{
      @include btn_pri; @include btn_large; 
      padding-left: 20px;
      padding-right: 20px;
      &:focus,
      &:hover{
        @include btn1;
      }
    }
    .group-button{
      @include clearfix;      
      .group_btn_right{
        float: right;
        ._btn{
          margin-left: $_padding_td - 2px;
        }
      }
    }
    .group-checkout-input{
      @include style_input();
      @include clearfix;    
      @include padding_container(15px);
      margin-top: 30px;
      @mixin _title{
        color: $color_heading;
        font-weight: $font-weight_normal;
        text-transform: uppercase;
        font-size: $_h4;
        line-height: 1em;
        @include respond-to($media_max_tablet){
          line-height: 1.6em;
        }       
      }
      .shipping-calculator{
        h3{
          @include _title;
          margin-bottom: 20px;
        }          
        .group_form{
          @include clearfix;
          .form-action{
            ._btn{
              float: left;
            }
          }            
        }    
        #wrapper-response{
          @include clearfix;
          margin-top: 30px;
          #shipping-rates-feedback{                
            margin-bottom: 0px;
          }
        }       
      }
      .checkout-buttons{          
        .note-content{
          label{
            @include _title;
            margin-bottom: 30px;
          }                   
        }        
      }
      @include respond-to($media_max_mobile){
        @include padding_container(0px); 
      }      
      textarea{
        height: 225px;
        margin-bottom: 5px;
      }
    }      
    .group_mobile{        
      .price-mobile,
      .quantity-mobile{
        margin-bottom: 5px;
      }
      .quantity-mobile{
        margin-top: 20px;
        .quantity-title{
          margin-right: 10px;
        }
      }
      .price-mobile{
        display:block;
      }        
    }
    @include respond-to($media_min_tabletsmall){
      .group_mobile{
        display: none;
      }
    }          
    @include respond-to($media_max_mobile){        
      table{             
        @include padding_container(10px);
        width: auto;       
        .product_name>a{
          font-size: $_h6;
          font-weight: $font-weight_normal;
          margin-bottom: 10px;
        }        
        thead{ display: none; }
        tbody {
          display: block;
          text-align: center;            
          @include display-flexbox();
          @include flex-wrap(wrap);
          @include align-items(flex-start);                                
          tr{                          
            padding: 0 10px;
            margin-bottom: 15px;
            vertical-align: top;
            display: block;
            border-width: 0;             
            .remove-mobile{
              margin-top: 20px;
              span{
                text-transform: uppercase;
              }
              .cs-icon{
                margin-right: 5px;
                &:before{
                  font-size: 12px;
                  font-weight: $font_weight_bold1;
                }
              }
            }
            &.summary{              
              width: 100%;
              @include clearfix;              
              td{                  
                width: $width_full / 2;
                display: block;
                float: left;
                padding: 15px;
                border: 1px solid $color_line;
                border-left-width: 0;
                border-right-width: 0;
                &.total_label{
                  font-size: $_h4;
                }
              }
            }              
            &:not(.summary){                  
              width: $width_full / 2; 
              margin-bottom: 30px;
              .money{
                font-size: $_h4;
                color: $color_heading;
                font-weight: $font_weight_normal;
              }
              td{                                  
                display: block;
                width: 100%;
                max-width: 100%;
                padding: 0 !important;                  
                &.remove, &.total.title-1, &.qty, &.price{
                  display: none !important; 
                  width: 0;
                  max-width: 0;
                  min-width: 0;
                }
                &.image{
                  max-width: 100%;  
                  min-width: 0;
                  margin-bottom: 25px;
                }
              }  
            }            
          }
        }

      }  
      .group-button{
        ._btn{
          width: 100%;
          margin-left: 0 !important;
          margin-bottom: 10px;
        }
        .group_btn_right{
          width: 100%;            
        }          
      }
      .group-checkout-input{
        > div{
          padding-left: 0;
          padding-right: 0;
        }
        .shipping-calculator{
          #wrapper-response{
            margin-bottom: 30px;
          }
        }
      }                
    }
    @include respond-to($media_max_mobilesmall){
      margin-top: $distance_section_subpage / 2;
      margin-bottom: $distance_section_subpage / 2;
      table {
        .product_name>a{
          font-size: $_h7 - 2px;
        }
        tbody tr:not(.summary) {
          width: 100%;
          .money{
            font-size: $_h7;
          }
          td{
            &.image{
              margin-bottom: 10px;
            }
          }
        }
      }

    }
  }  
