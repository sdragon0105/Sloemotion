  .blog-layout{
    .articles-group {
      @include clearfix;
      margin-left: -15px;
      margin-right: -15px;
      .articles-group-inner{
        &:not(.masonry){
          @include display-flexbox();
          @include flex-wrap(wrap);
        }        
      }
    }    
    .blog-content{       
      &.blog-sidebar{       
        .articles-group{
          margin-left: -15px;
          margin-right: -15px;
        }
        .articles-group-inner{          
          @include display-flexbox();
          @include flex-wrap(wrap);            
          > div{
            @include respond-to($media_max_tabletsmall){
              width: 50%;              
            }
            @include respond-to($media_max_mobile){
              width: 100%;
            }
          }
        }
      }
      &.blog-default{
        .articles-group-inner{
          > div{
            @include respond-to($media_max_tabletsmall){
              width: 50%;              
            }
            @include respond-to($media_max_mobile){
              width: 100%;
            }
          }  
          &.masonry { 
            -webkit-column-count: 3; /* Chrome, Safari, Opera */
            -moz-column-count: 3; /* Firefox */
            column-count: 3;            
            column-gap: 30px;
            -moz-column-gap: 30px;
            -webkit-column-gap: 30px;                        
            padding: 0;          
            .item {           
              @include inline-block();              
              width: 100%;              
              margin-bottom: 40px;
              overflow: hidden;
              .article{                                
                margin-bottom: 0 !important;                                
                .article-image{
                  > a{
                    display: block;
                    position: relative;
                    &:after{
                      content: '';
                      position: absolute;
                      top: 0;
                      left: 0;
                      right: 0;
                      bottom: 0;
                      $__color: #000;
                      background-color: hsla(hue($__color), saturation($__color), lightness($__color), 0);
                      @include transition();
                      @include opacity(0,0);
                    }
                  }
                }
                &:hover{
                  .article-image>a:after{
                    @include opacity(1,100);
                    top: 50%;
                    left: 50%;
                    @include transform(translate(-50%, -50%));
                  }
                }
              }              
            }
            @include respond-to($media_max_tabletsmall){
              -webkit-column-count: 2; /* Chrome, Safari, Opera */
              -moz-column-count: 2; /* Firefox */
              column-count: 2;   
            }
            @include respond-to($media_max_mobile){
              -webkit-column-count: 2; /* Chrome, Safari, Opera */
              -moz-column-count: 2; /* Firefox */
              column-count: 2;   
            }
            @include respond-to($media_max_mobilesmall){
              -webkit-column-count: 1; /* Chrome, Safari, Opera */
              -moz-column-count: 1; /* Firefox */
              column-count: 1;   
            }
          }          
        }
      }
      .pagination_group{
        margin: $distance_section_subpage auto $distance_section_subpage;
      }
    }
  }
