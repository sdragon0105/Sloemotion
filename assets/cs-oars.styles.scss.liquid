      a{
        font-size: $_h7;
        color: $color_body;
        line-height: 1.5em;                
        text-transform: capitalize;
        font-weight: $font_weight_thin;           
        @include transition();
        &:hover, &:focus{
          color: $color_primary;          
        }           
      }
      &:hover, &:focus,
      &.active {
