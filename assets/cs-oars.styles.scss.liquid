body{ 
  @mixin slick_dots_style1($color_dot: #646464){    
    @include display-flexbox();
    @include flex-wrap(wrap);  
    @include flex-direction(column);
    position: absolute;
    right: 70px;
    top: 50%;                
    left: auto;
    bottom: auto;
    width: auto !important;
    margin: 0 !important;        
    @include transform(translateY(-50%));        
    li {
      margin: 5px;
      width: auto !important;
      button{            
        border-radius: 50%;
        overflow: hidden;            
        width: 24px;
        height: 24px;
        position: relative;
        background-color: transparent;            
        border-color: transparent;
        border-width: 1px;
        border-style: solid;
        @include transition();
        &:focus{
          outline: none;
        }
        &:before{
          position: absolute;
          left: 50%;
          top: 50%;
          width: 5px;
          height: 5px;                        
          @include transform(translate(-50%, -50%));
          border-radius: 50%;
          overflow: hidden;            
          background-color: $color_dot;
        }
      }  
      &:hover,
      &.slick-active{
