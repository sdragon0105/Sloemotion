@mixin menu-label (){
  position: absolute !important;
  text-transform: none;
  font-size: $_h7 - 2px;
  font-weight: $font_weight_bold;
  letter-spacing: 0.05em;
  padding: 2px 7px;
  top: auto;
  bottom: 100%;
  margin-bottom: 10px;
  right: 0;
  display: block;
  line-height: 16px;    
  @include transform(translateX(50%));
  &:after{
    position: absolute;
    content: '';
    display: block;
    top: 100%;
    left: 50%;
    @include transform(translateX(-50%));            
    border-left: 0 solid transparent;
    border-right: 6px solid transparent;      
  }
  @include respond-to($media_max_mobile){
    margin-bottom: 0px;
  }
}
@mixin menu-label-color ($var1: $color_sub, $var2: #fff){
  color: $var2 !important;
  background-color: $var1 !important; 
  &:after{
    border-top: 6px solid $var1;
  }
}
.submenu-label,
.menu-label{
  @include menu-label();
  &.label-sale{
    @include menu-label-color($color_newlabel, #fff);
  }
  &.label-hot{
    @include menu-label-color($color_bestlabel, #fff);
  }
}
