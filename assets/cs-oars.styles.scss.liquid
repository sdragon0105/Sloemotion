              padding: 0 !important;
              border-top-width: 0;
              @mixin _spr_pagination_style_btn{
                @include _btn;
                @include btn_width(32px, 1px);
                @include btn_color($color_tertiary, $color_line, #fff);               
                border-radius: 0;
                text-align: center;
                width: 32px;                  
                overflow: hidden;
                padding: 0 !important;
