      @extend .icon-small-angle-right;
    }
    .fb-prev span{
      @extend .icon-small-angle-left;
    }
    .fb-close{
      top: 15px;
      right: 15px;      
      .cs-icon{
        $_font_size: 12px;
        font-weight: $font_weight_boldmedium;
        color: $color_heading;
        font-size: $_font_size;
        position: absolute;
        top: 50%;     
        left: 50%;
        margin-left: - $_font_size / 2;          
        margin-top: - $_font_size / 2; 

      }
    }
    @include respond-to($media_max_tablet){
      .fb-nav span{
        visibility: visible;
      }
    }
  }
  .wishlist-model{
    @include model_wishlist_compare;
  }
  .compare-model{
    @include model_wishlist_compare;
  }
  /*End wishlist_compare*/  
