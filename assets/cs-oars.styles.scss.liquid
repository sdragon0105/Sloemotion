/*SUB PAGE*/
body{
  &:not(.index-template){
    background-color: $color_content_bg;
    .home-process-layout{
      padding: 0 !important;
    }
    .home-about-layout{
      padding: 0 !important;
    }
    .home-deal-layout,
    .home-core-layout{
      margin-bottom: - $distance_section*2;
    }
  }
  @mixin _sub_title{
    color: $color_heading;
    font-weight: $font_weight_normal;
    font-size: 20px;
    text-transform: uppercase;
    margin-bottom: 15px;
  }
  &:not(.index-template){
    .sub-title{
      text-align: center;   
      *{
        @include _sub_title;
      }
    }
    main{
      .shopify-section{
        padding-top: $distance_section_subpage;
        padding-bottom: $distance_section_subpage;        
      }
    } 
    .index-section.index-section-banner{           
      padding: 0;
    }
  } 
  /*SIDE BAR*/  
  $___max_width_sb: 300px;
  @mixin sb_title($___font_size: 16px, $border_width: 1px){          
    position: relative;          
    padding-right: $___font_size + 5px;  
    padding-top: 5px;
    padding-bottom: 5px;
    margin: 0; 
    margin-bottom: 5px;
    .text{
      text-transform: capitalize;
      color: $color_heading;
      font-size: $___font_size;
      display: block;
      line-height: 2em;      
      padding-bottom: 5px;
      padding-top: 5px;
      font-weight: $font_weight_bold1;
      position: relative;
      $_width_line: 31px;
      $_width_dote: 2px;
      &:before,&:after{
        position: absolute;
        content: '';
        left: 0;
        top: 100%;
        height: 2px;
        background-color: $color_primary;
      }
      &:before{
        width: $_width_line;
      }
      &:after{
        width: $_width_dote;
        margin-left: $_width_dote + $_width_line;
      }
    }
    .cs-icon{
      position: absolute;
      right: 0;
      top: 50%;
      @include transform(translateY(-50%));
      text-align: right;            
      font-size: 0;
      cursor: pointer;
      &:before{              
        text-align: center;
        font-weight: $font_weight_bold;
        color: $color_heading;               
        font-size: $___font_size
      }
    }
  }
  @mixin sb_linklist_line_style(){
