    /*home-productTab-layout*/
    .home-productTab-layout{ 
      margin-bottom: $distance_section - $distance_product;
      .page-title{        
        margin-bottom: 18px;
      }
      .home-product-inner{
        margin-left: -15px;
        margin-right: -15px;
      }      
      .product-tab{               
        overflow: hidden; 
        .nav-tabs{
          $____height: 30px;
          position: relative;                        
          text-align: center;
          margin-bottom: $distance_section;
          >li{
            float: none;
            @include inline-block();
            margin: 4px;
            > a{
              height: $____height;                    
              line-height: $____height - 2px;
              border: 1px solid $color_line;
              font-size: 13px;
              font-weight: $font_weight_bold1;              
              border-radius: 30px;               
              -moz-background-clip: padding;
              -webkit-background-clip: padding-box;
              background-clip: padding-box;              
              padding: 0 20px;              
              text-transform: uppercase;                
              background-color: transparent;
              @include transition();
            }
            &:hover, &:focus,
            &.active{
              > a{
                color: #fff;                
                background-color: $color_primary;
              }
            }
            &.tab-hide{
              display: none;
            }
            &.tabs-dropdown{
              visibility: hidden;
              display: none;
              &.active_dropdown{
                visibility: visible;
                position: absolute;
                @include inline-block();
                right: 0;                 
                top: 50%;                  
                z-index: 1;
                @include transform(translateY(-50%));
                > a{                     
                  width: $____height;
                  height: $____height;                    
                  line-height: $____height - 2px;
                  text-align: right;
                  cursor: pointer;
                  .cs-icon{                        
                    line-height: $____height;
                    font-size: 0;
                    &:before{
                      font-size: 34px;
                      color: $color_heading;
                    }
                  }
                }
                > .dropdown-menu{
                  display: block;
                  visibility: hidden;
                  @include transition();
                  @include transform(translateX(100%));
                  position: absolute;
                  top:100%;
                  right: 0;
                  left: auto;
                  background-color: hsla(hue($color_altenative), saturation($color_altenative), lightness($color_altenative), 0.9);
                  margin-bottom: 0;
                  text-align: right;
                  padding: 0;
                  > li{
                    display: none;
                    margin: 0;
                    padding: 10px 15px;                        
                    &:not(:last-child){
                      $__color: #fff;
                      border-bottom: 1px solid hsla(hue($__color), saturation($__color), lightness($__color), 0.9); 
                    }                        
                    &.tab-show{
                      display: block;
                      > a{
                        font-size: $_h6;
                        border-width: 0;
                        color: $color_heading;
                      }
                    }
                    > a:hover, > a:focus,
                    &.active > a{
                      color: $color_sub;
                    }
                    > a{
                      font-weight: $font_weight_normal;                          
                      color: $color_heading;
                    }
                  }
                }
                &.open{                        
                  >a, 
                  >a:focus, 
                  >a:hover{
                    background-color: transparent;
                    border-width: 0;                        
                    .cs-icon::before{
                      color: $color_sub;
                    }
                  }                    
                  > .dropdown-menu{ 
                    visibility: visible; 
                    @include transform(translateX(0px));                      
                  }
                }
              }
            }            
          }

        }
      }     
      .tab-content{        
        .product-item{
          @include display-flexbox();                    
          @include flex-wrap(wrap);                     
        }
      }
      @include respond-to($media_max_tabletsmall){
        .content-product{
          width: $width_full / 3;
        }
      }
      @include respond-to($media_max_mobile){
        .content-product{
          width: $width_full / 2;
        }
      }
      @include respond-to($media_max_mobilesmall){
        .home-product-inner{
          margin-left: -5px;
          margin-right: -5px;
        }      
        .content-product{
          padding-left: 5px;
          padding-right: 5px;
        }
      }
    }
    /*end home-productTab-layout*/
    /*home-productCol-layout*/
