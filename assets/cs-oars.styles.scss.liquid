  @mixin sb_linklist_style(){            
    li{       
      padding-top: 7px;
      padding-bottom: 7px;
      &:not(:last-child){
        border-bottom: 1px solid hsla(hue($color_line), saturation($color_line), lightness($color_line), 0.4);
