                @include btn4;
                font-weight: $font_weight_normal;
              }
              .spr-pagination-page{   
                @include _spr_pagination_style_btn;
                &:hover,
                &.is-active{
                  @include _spr_pagination_style_btn_hover;                  
                }
              }
              .spr-pagination-next,
              .spr-pagination-prev{   
                top: 0;
                span,
                a{                  
                  @include _spr_pagination_style_btn;                                    
                  position: relative;
                  font-size: 0;
                  &:before{
                    @include fontawesome();
                    position: absolute;
                    top: 0;
                    bottom: 0;
                    left: 0;
                    right: 0;
                    line-height: 30px;
                    text-align: center;
                    text-indent: 0;
                    font-size: $_h5;
                  }
                }                  
                &:hover{
