  /*PRODUCT DETAIL PAGE*/
  $height_wp: 38px;
  $width_wp: 100%;
  $width_input: 100%;
  $width_label: 60px;
  $color-detail-title: $color_tertiary;     
  $____color_border: #ddd;
  $_width_max_quality: 90px;
  $width_product_type: 90px;
  $_distance_title_label: 10px;
  @mixin _detail-title{
    margin-top: 0; 
    font-family: $font_1;
    text-transform: uppercase;
    color: $color-detail-title;
    font-weight: $font_weight_bold;     
    font-size: 20px;    
  }
  @mixin _label-detail{    
    font-family: $font_1;
    font-size: $_h7 + 1px;
    font-weight: $font_weight_thin;
    color: $color_body;
    text-transform: capitalize;                                                                
  }
  /* swatch */
  .no-options .swatch{ display:none !important; }  
  .swatch{
    $height_swatch: 40px;
    $width_swatch: 40px;
    $height_swatch_color: 40px;
    $width_swatch_color: 40px;
    $border_width_swatch: 1px;     
    margin-bottom: 10px;
    margin-top: 10px;
    .header{
      @include _label-detail;            
      margin-bottom: 5px;
    } 
    .swatch-element{ 
      float: left;                           
      border-width: 0px;        
      @include transition();          
      position: relative;    
      margin-top: 2px !important;
      margin-bottom: 2px !important;
      margin-right: 5px !important;
      .crossed-out{
        display: none;
      }
      input{
        display: none;
      }            
      label{
        width: 100%;
        height: 100%;
        padding-left: 15px;
        padding-right: 15px;
        border: $border_width_swatch solid $____color_border;
        background-color: transparent;                                                    
        font-weight: normal;
        text-align: center;
        line-height: $height_swatch - $border_width_swatch*2 - 1px;
        background-position:-1px -1px;          
        font-size: $_h7;
        cursor: pointer;
      }      
      input:checked + label,      
      &.active label{
        cursor: default;
      }                               
    }  
    &:not(.color){
      $border-radius: 0;                 
      .swatch-element{ 
        min-width: $width_swatch;
        height: $height_swatch;        
        border-radius: $border-radius;
        background-position: -1px -1px;
        margin-right: -1px;        
        label{
          border-radius: $border-radius;          
        }
        input:checked+label,                 
        &.active label,         
        &:hover label{
          background-color: $color_primary;
          color: #fff;          
        }         
      }
    }
    &.color{
      $border-radius: 0%;                      
      padding: 0px;   
      cursor: pointer;      
      .swatch-element{ 
        min-width: $width_swatch_color;
        height: $height_swatch_color;        
        border-radius: $border-radius;
        border-width: 0;        
        label{
          border-radius: $border-radius;                    
          border-width: 0 !important;
          margin: 0 !important;
        }        
        input:checked + label,      
        &.active label,
        &:hover label{                
          &:before{
            $____font_size: $_h6;
            @include fontawesome();
            content: "\f00c";
            $__color: #fff;
            color: hsla(hue($__color), saturation($__color), lightness($__color), 0.9);
            position: absolute;
            top: 50%;
            left: 0;
            right: 0;
            text-align: center;
            font-size: $____font_size;
            margin-top: - $____font_size / 2;
          }
        }     
      }                                
    }
    @include respond-to($media_max_mobilesmall){
      .header{ font-size: $_h7 -2px;}
      .swatch-element{
        margin-bottom: 7px;
      }
    }
  }          
  /*End swatch*/  
  @mixin _detail-description{
    font-size: 14px;
    color: $color_product_description_text;
    line-height: 1.8em;
    font-weight: $font_weight_thin;
  }
  @mixin _detail-money{    
    color: $color_price_text_detail;    
    font-family: $font_1;    
  }
  @mixin _detail-money-del{    
    color: $color_price_compare_text_detail; 
    .money{
      color: $color_price_compare_text_detail;
    }
  }
  /*product-type */
  @mixin _product-type{        
    .selector-wrapper{            
      margin-bottom: 20px;      
      position: relative;
      border-bottom: 1px solid $color_line;
      padding-left: $width_label;
      label{     
        @include _label-detail;          
        width: $width_label;
        line-height: 1em;         
        margin-bottom: 0 !important;
        position: absolute;
        top: 50%;
        left: 0;
        @include transform(translateY(-50%));
      }
      select{        
        padding-left: 10px;
        padding-right: 10px;                       
        background-color: #fff;                    
        border: 1px solid $color_line;            
        text-align: left;
        box-shadow: none;
        border-radius: 0;  
        width: 100%;
        height: $height_wp; 
        max-width: 100%;
        text-transform: capitalize;
        color: $color_heading;
        border-width: 0;                                
      }
    }
  }      
  /*Quantity wrapper    */
  @mixin _quantity-wrapper{     
    .wrapper-title{
      @include _label-detail;           
    }   
    .wrapper{
      $_wp_color_bg: #fff;
      @include display-flexbox();
      @include flex-wrap(nowrap);
      @include align-items(center); 
      height: $height_wp + 2px;
      min-width: auto;      
      max-width: 127px;
      padding: 0 ($height_wp / 2) 0 ($height_wp / 2);
      border-radius: $border_radius_input;      
      position: relative;
      border: 1px solid $color_line;
      background-color: $_wp_color_bg;
      border-radius: 5px;        
      .qty-up,
      .qty-down{     
        @include fonticomoon();
        position: absolute;                    
        text-align: right;
        width: 25px;
        height: $height_wp;          
        line-height: $height_wp;
        top: 50%;
        text-align: center;
        cursor: pointer;        
        @include transform(translateY(-50%));        
        &.cs-icon {          
          color: $color_heading;          
          font-weight: $font_weight_normal;
          font-size: $_h7 - 2px;
        }
      }      
      .qty-down{
        left: 0;
        &:before{
          content: '\e919';
        }
      }
      .qty-up{ 
        right: 0;
        &:before{
          content: '\e91b';
        }
      }
      input[type="text"]{
        text-align: center;       
        border-width: 0;
        font-size: $_h7;   
        height: 100%;        
        padding: 5px;
        color: $color_body;
        font-weight: $font_weight_normal;
        font-family: $font_1;
        background-color: transparent;        
      }   
      @include placeholder(){
        font-size: $_h6 - 1px;
        color: $color_heading;
      }                
    }
  }    
  /*gallery images style*/    
  @mixin __btn_slider_general($_padding: 15px, $_width: 30px){    
    .owl-prev,
    .owl-next{
      @include display-flexbox();      
      @include align-items(center);      
      @include justify-content(center);
      text-align: center;
      height: 100%;
      width: $_width;
      height: $_width;
      position: absolute;                          
      top: 50%;
      @include transform(translateY(-50%));      
      @include btn4;
      &:hover,
      &:focus{ @include btn1; background-color: transparent; }
    }
    .owl-prev{ left: $_padding + 5px; }
    .owl-next{ right: $_padding + 5px; }
  }  
  @mixin gallery_images_style($_padding_gallery_images: 5px, $_width_button: 30px){                   
    .show-image-load {    
      text-align: center;                          
      width: 100%;
      z-index: 9;         
      i{ color: $color_primary;}
    }  
    .gallery-images-inner{
      margin-left:  - $_padding_gallery_images;
      margin-right:  - $_padding_gallery_images;
    }    
    .featured {
      img{
        border: 1px solid $color_line;
      }
    }
    .vertical-image-content{      
      .image-vertical{
        padding-right: $_padding_gallery_images;
        padding-left: $_padding_gallery_images;
        text-align: center;
        a{        
          display: block;            
          padding: 3px 0;
          {% if settings.main_image_display == 'zoom' %}
          &.active{                    
            img{                                            
              opacity: 0.4;
              filter: alpha(opacity=40); 
            }
          }
          {% endif %}
        } 
        &:hover,
        &.active{                           
          img{                                            
            border: 1px solid hsla(hue($color_primary), saturation($color_primary), lightness($color_primary), 1);
            opacity: 0.6;
            filter: alpha(opacity=60); 
          }
        }            
      }        
      .owl-controls{
        @include __btn_slider_general($_padding_gallery_images, 30px);                                                 
      }
      &:focus,
      &:hover{
        .owl-controls{
          @include opacity(1,100);
        }
      }
    }            
  }
  /*end gallery images style*/
  /* product information style*/
