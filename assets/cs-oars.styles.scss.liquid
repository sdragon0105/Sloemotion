/*MAIN CONTENT*/
@mixin blog_style(){
  .blog-title-group{
    text-align: center;
    padding: 15px 0;
  }
  .blog-title{
    @include inline-block();
    margin: 0 auto;
    text-align: center;
    background-color: $color_primary;
    text-transform: uppercase;
    color: #fff;
    font-size: 12px;
    padding: 0px 5px;
    line-height: 16px;
    font-weight: $font_weight_thin;
  }
  .article-image{   
    a{
      display: block;
      overflow: hidden;
      position: relative;
      &:after{
        content: '';
        position: absolute;
        top: 50%;
        left: 50%;
        width: 55px;
        height: 55px;
        border: 2px solid #fff;
        border-radius: 50%;
        @include transform(translate(-50%, -50%));
        background-image: url({{ 'icon_blog_bg.png' | asset_url }}); 
        background-size: auto;
        background-repeat: no-repeat;
        background-position: center center;
        @include transition();
        @include opacity(0,0);
      }
      &:hover{
        &:after{
          @include opacity(1,100);
        }
      }
    }
  }
  .article-name{    
    text-align: center;
    font-size: 0;
    margin-bottom: 15px;
    a{
      font-size: 14px;
      color: $color_heading;
      font-weight: $font_weight_bold1;
      &:hover{
        color: $color_primary;
      }
    }
  }
  .article-info{
    @include display-flexbox();
    @include flex-wrap(wrap);                                    
    @include justify-content(center);
    @include align-items(center);
    li{
      display: block;      
      position: relative;  
      padding: 0 15px;
      &:not(:last-child){
        padding-right: 7px + 15px;
        &:before{
          content: '';
          position: absolute; 
          right: 0;
          top: 50%;
          width: 7px;
          height: 7px;
          border-radius: 50%;
          background-color: hsla(hue($color_sub), saturation($color_sub), lightness($color_sub), 0.5);                               
          @include transform(translateY(-50%));
        }
      }
    }
    *{
      font-size: 12px;
      color: $color_body;
    }
  }  
  .articleinfo_group{
    background-color: hsla(hue($color_line), saturation($color_line), lightness($color_line), 0.5);
    padding: 0px 15px ($distance_section - 10px);
  }
  .article {
    margin-bottom: 30px;
    @include transition();
    @include respond-to($media_min_desktopsmall){
      &:hover{
        @include box-shadow(0px 12px 16px rgba(0,0,0,0.12));
      } 
    }    
  }  
}
