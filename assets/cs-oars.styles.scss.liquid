._alert {
  $_width_alert: 570px;  
  $_border_width_alert: 1px;
  $_padding: 17px;
  $_icon_width: 92px;
  $_font_size_close: 16px;    
  width: 100%; margin-bottom: 30px;  
  max-width: $_width_alert;
  font-size: $font_size_base;  
  background-size: auto;
  background-position: 12px center;
  background-repeat: no-repeat;
  border-style: solid;
  border-width: 1px;    
  position: relative;
  line-height: 1em;      
  padding: 16px ($_padding + $_font_size_close) 16px (2*$_padding + $_icon_width);  
  @include un_style_ul;    
  .close{        
    position: absolute;
    top: 50%;
    right: $_padding;
    font-size: $_font_size_close;
    margin-top: - $_font_size_close / 2;
    font-weight: $font_weight_bold;
    @include text-shadow(none);
    @include opacity(1, 100);
    &:hover{
      @include opacity(0.7, 70);
    }
  }
  &.alert_success{ @include btn_color($color_success_text, $color_success_border, $color_success_bg); background-image: url({{ 'bg_alert_success_icon.png' | asset_url }}); }
  &.alert_cross{ @include btn_color($color_cross_text, $color_cross_border, $color_cross_bg); background-image: url({{ 'bg_alert_notice_icon.png' | asset_url }}); }
  &.alert_fail{ @include btn_color($color_fail_text, $color_fail_border, $color_fail_bg); 
    background-image: url({{ 'bg_alert_failure_icon.png' | asset_url }}); }
  &.aler_addtocart{
    @include btn_color($color_addtocart_text, $color_addtocart_border, $color_addtocart_bg);
    background-image: url({{ 'bg_addtocart_icon.png' | asset_url }});
