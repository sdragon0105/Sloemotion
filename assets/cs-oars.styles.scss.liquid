          .tab-content{
            #customer_review{
              .preview_content{
                .spr-container{ 
                  .spr-content{
                    .spr-reviews{
                      .spr-review{
                        $_width_review_header: $width_full / 3;                  
                        .spr-review-header{
                          width: $_width_review_header;
                        }
                        .spr-review-content{
                          width: $width_full - $_width_review_header;
                        }
                        .spr-review-footer{
