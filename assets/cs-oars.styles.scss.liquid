  $__distance_seletor: 10px;
  margin-bottom: $distance_product;
  overflow: hidden;  
  box-sizing: border-box;  
  position: relative;  
  border-style: solid;
  border-width: 1px;
  border-color: $color_product_border;   
  text-align: center;  
  background-color: $color_product_bg;     
  @include transition();
  .rating-star{
    margin-bottom: $__distance_seletor;
    .spr-badge-starrating{
      margin: 0;
    }
    .spr-badge-caption{
      display: none !important;
    }
  }
  .product-price{                         
    background-color: transparent;                  
    .price,
    .price_sale,
    .price_compare{
      font-family: $font_1;
    }
    .price,
    .price_sale{
      color: $color_price_text;     
      font-size:  $font_size_price;
      font-weight: $font_weight_normal;
    }
    .price_compare{
      color: $color_price_compare_text;
      margin-left: 5px;      
      font-size:  $font-size-price - 1px;  
      font-weight: $font_weight_normal;      
      vertical-align: top;
    }
  }
  .product-title{ 
    margin-bottom: $__distance_seletor;    
    margin-top: 0px;    
    a{
      @include inline-block(); 
      @include transition();
      line-height: 1.5em;      
      text-transform: capitalize;
      font-weight: $font_weight_bold1;                     
      font-size: $font_size_title_pro;
      color: $color_product_title_text;
      font-family: $font_1; 
      &:hover, &:focus{
        color: $color_product_title_text_hover;
      }
    }                      
  }      
  .hover-mask{
    @include display-flexbox();
    @include flex-wrap(nowrap);    
    @include justify-content(center);      
    width: auto;
    position: absolute;
    z-index: 1;
    top: 0;
    left: 50%;
    right: auto;
    bottom: auto;
    text-align: center; 
    background-color: $color_content_bg;
    border-radius: 30px;
    padding-left: 5px;
    padding-right: 5px;
    border: 1px solid hsla(hue($color_line), saturation($color_line), lightness($color_line), 0.5);
    .tooltip{
      white-space: nowrap;
    }
    @include box-shadow(0px 12px 16px rgba(0,0,0,0.12));
    @include transform(translate(-50%,-50%));
    @include opacity(0, 0);
    @include transition();
    @include respond-to($media_max_mobile){
      @include opacity(1, 100);            
    }
  }
  .row-right{
    @include transition();           
    text-align: center;     
    padding-top: 2*$__distance_seletor + 5px;
    padding-bottom: 2*$__distance_seletor + 5px;
    padding-left: 15px;
    padding-right: 15px;
    position: relative;
  }     
  .row-left{    
    overflow: hidden;    
    font-size: 0;    
    position: relative;
    text-align: center;
    > a{            
      @include inline-block();
      font-size: 0;                  
      text-align: center;
      position: relative;
      img{
        &.rotation {
          position: absolute;
          top: 0;
          left: 0;
        }
      }
    }     
  }    
  .quickview-wishlist-wrapper{
    @include display-flexbox();
    @include flex-wrap(nowrap);    
    @include justify-content(center);      
    width: auto;

    li{
      position: relative;
      &:before{
        content: '';
        width: 2px;
        height: 15px;
        position: absolute;
        left: 0;
        top: 50%;
        @include transform(translateY(-50%));
        background-color: hsla(hue($color_line), saturation($color_line), lightness($color_line), 0.4);
      }
    }
  }
  .wishlist a,
  .effect-ajax-cart button,  
  .quickview a{
    @include btn-quickview-wishlist;
    cursor: pointer;      
  }      
  .wishlist a, 
  .effect-ajax-cart button, 
  .quickview a{
    padding-left: 10px;
    padding-right: 10px;
    &:hover{
      color: $color_primary;
    }
    @include respond-to($media_min_desktopsmall){
      &:focus{
        color: $color_primary;
      } 
    }
  }
  .wishlist a{
    &:hover,
    &.wishlist-added,
    &.wishlist-added:hover{
      color: $color_primary;
    }
    @include respond-to($media_min_tabletsmall){              
      &:focus,
      &.wishlist-added:focus{
        color: $color_primary;
      }
    }
  }

  @include respond-to($media_min_tabletsmall){        
    .row-left>a img.rotation,                
    .quickview,
    .effect-ajax-cart,    
    .wishlist,
    .compare{      
      @include opacity(0,0);      
      @include transition();      
    }
    .effect-ajax-cart{
      @include transform(translateY(5px));      
    }    
    .quickview{
      @include transform(translateY(-5px));      
    }
    .wishlist{
      @include transform(translateY(10px));      
    }
    .compare{  
      @include transform(translateY(-10px));      
    }
    .product-wishlist{
      @include transform(translate(0px, 0px));            
      @include transition();      
    }    
    &:hover,
    &:focus{       
      @include box-shadow(0px 12px 16px rgba(0,0,0,0.12));
      .row-left>a img.rotation{
        @include transition-delay(1s);
        @include opacity(1,100);
      }            
      .effect-ajax-cart,      
      .quickview,
      .wishlist,
      .compare{            
        @include transform(translateX(0%));      
        @include transform(translateY(0%));      
        @include opacity(1,100);
      }  
      .product-wishlist{
        @include transform(translate(-5px, 5px));              
      }
      .hover-mask{
        @include opacity(1, 100);
      }
    }
  }
  @include respond-to($media_max_mobile){    
    .quickview{
      display: none !important;
    }
    .row-left{
      > a{
        img:nth-child(2){
          display: none;
        }
      }
    }    
  }
  @include respond-to($media_max_mobilemedium){
    margin-bottom: $distance_product / 2;   
  }
}

