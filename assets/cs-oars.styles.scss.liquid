        .home-testimonials-inner{
          margin-left: -15px;
          margin-right: -15px;
        }
        .home-testimonials-content{
          .testimonials-item{                
            width: $width_full / 2;
            padding-bottom: 15px;
            padding-top: 15px;
            $_width_img: 30%;
            .group{
              @include display-flexbox();
              @include flex-wrap(wrap);
              @include align-items(center); 
              background-color: $color_content_bg;
            }
            .group_img{
              max-width: $_width_img;
