  .compare-page{
    #comparecontent{
      margin-top: $distance_section_subpage * 2;
      margin-bottom: $distance_section_subpage * 2;
      @include respond-to($media_max_mobilemedium){
        margin-top: $distance_section_subpage;
        margin-bottom: $distance_section_subpage;
      }
      span.none{
        text-transform: uppercase;
        text-align: center;
      }
      ul{
        margin: 0;
        li{
          list-style: none;
        }
      }
      .compare-count{
        text-align: center;        
        margin-bottom: 30px;
        text-transform: uppercase;        
        display: block;
        font-weight: $font_weight_bold;
      }
      .compare-heading{
        display: none;
      }
      > ul{
        @include display-flexbox();
        @include flex-wrap(nowrap);            
        padding-left: $width_full / 4;        
        .compare {          
          &:nth-of-type(2){
            > div{                           
              &:not(.compare-remove){
                position: relative;
                &:before{
                  @include  _heading_style_tb;
                  position: absolute;                  
                  width: 100%;
                  right: 100%;
                  top: 0;
                  bottom: 0;                                    
                  text-align: right;
                  padding-left: 0;
                  padding-right: 15px;
                  padding-top: 17px;
                  padding-bottom: 17px; 
                  margin-bottom: -1px;
                }  
              }              
            }
            .compare-image{
              &:before{
                content: "product";                
              }
            }  
            .compare-addCart{
              &:before{
                content: "add to cart";                
              }
