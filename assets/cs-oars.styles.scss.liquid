  @mixin __search_form($var: 40px){
    form{
      position: relative;      
      padding-right: $var;
      height: $var;
      input[type="text"]{
        width: 100%;
        padding: 15px;        
        background-color: transparent;
        @include box-shadow(none);
        border-width: 0;
        height: 100%;
        line-height: $var;
      }
      button[type="submit"]{
        position: absolute;
        top: 0;
        right:0;
        width: $var;
        height: $var; 
        line-height: $var; 
        @include appearance(none);
        border-width: 0;
        background-color: transparent;
        color: $color_heading;        
        *{
          font-weight: $font_weight_boldmedium;
          line-height: $var;
        }
      }
    }
