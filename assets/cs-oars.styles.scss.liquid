    /*home-about-layout*/
    .home-about-layout{ 
      padding-bottom: $distance_section * 2 !important;
      .home-about-inner{
        margin-left: -15px;
        margin-right: -15px;
      }
      .home-about-content{
        @include display-flexbox();                    
        @include flex-wrap(wrap);         
        .about-item{
          width: $width_full / 4;
          text-align: center;
          img{
            width: 150px;    
            max-width: 100%;
            margin-bottom: 28px;
          }
