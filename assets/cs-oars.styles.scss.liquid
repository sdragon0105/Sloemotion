  /*LOOKBOOK PAGE*/
  .page-lookbook{
    .lookbook-content-inner{ @include padding_container();}
    .lookbook-content{
      @include display-flexbox();
      @include flex-wrap(wrap); 
      .lookbook-item{
        width: $width_full / 2;
        padding: 15px;
        .item{          
          .lb-element{
            a{
              display: block;
              position: relative;
              &:before{
                content: '';
                position: absolute;
                top: 0;
                bottom: 0;
                left: 0;
                right:0;
                @include style_overlay();
                @include transition();
                visibility: hidden;
                @include opacity(0,0);
              }
              &:hover, &:focus{
                &:before{
                  visibility: visible;
                  @include opacity(1,100);
                }
              }
            }
          }          
        }
      }     
    }
  }  
  /*END LOOKBOOK PAGE*/
