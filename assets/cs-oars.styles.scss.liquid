  /*CUSTOMER PAGE*/
  @mixin account_table_style{
    table{
      thead{
        tr{
          background-color: $color-primary;
          padding: 10px 0;
          th{
            @include account_title_style;                  
            color: #fff;
            padding: 10px 30px;
            white-space: nowrap;
          }
        }                
      }
      tbody{
        font-size: $_h7;
        border: 1px solid $color_line;                
        *{
          color: $color_heading;
        }
        tr{
          border-bottom: 1px solid $color_line;
          td{                    
            padding: 23px 30px;
            &.td-note{
              white-space: nowrap;
            }
          }
          &:last-child{
            border-bottom-width: 0;
          }
        }
      }
      tfoot{
        font-size: $_h7;
        color: $color_heading;
        td{
          border: 1px solid $color_line;
          padding: 10px 30px;
        }
      }
    }
  }
  @mixin account_title_style{
    font-size: $_h7;
    font-weight: $font_weight_boldmedium;
    text-transform: uppercase;
    @include respond-to($media_max_mobilesmall){
      font-size: $_h7;
    }
  }
  @mixin details_item($_width_title: 100px){
    margin-bottom: 10px;                
    @include display-flexbox();
    @include align-items(center);
    .title{                  
      color: $color_heading;
      width: $_width_title;
      max-width: $_width_title;
      padding-right: 10px;
      @include inline-block();
      white-space: nowrap;
    }
    &.name{
      .content{
        font-weight: $font_weight_bold;
      }
    }
    address{
      @include inline-block();
      margin-bottom: 0;
    }
    &:last-child{
      margin-bottom: 0;
    }     
    @include respond-to($media_max_mobilesmall){
      *{
        font-size: $_h7;
      }
    }
  }
  $account_style_width: 800px;
  @mixin inner_content_wrapper{
    width: 100%;
    max-width: $account_style_width;            
    margin: 0 auto;
  }
  @mixin form_address_style{
    $_padding_input: 13px;
    $_padding_group_form: 50px;
    form{           
      .customer-address-table{
        @include padding_container($_padding_input);              
      }
      #add_address_title{
        display: none;
      }            
      .group_form{
        padding: $_padding_group_form;
        border: 1px solid $color_line;              
        margin-top: 30px;
        .form-item{
          width: $width_full / 2;                
          padding: 0 $_padding_input;
          float: left;
          label{
            text-align: left;
          }
          .form-item-inner{
            @include display-flexbox();
            @include align-items(center);
            @include flex-wrap(nowrap);
            @include justify-content(flex-start);   
          }
          &:last-child{
            width: 100%;
            float: none;
            text-align: left;
            *{
              text-align: left;
            }
          }
        }
      }    
      .action_bottom{
        @include clearfix;
        @include display-flexbox();
        @include flex-wrap(nowrap);
        @include align-items(center);               
        ._btn{
          margin: 0 5px 0 0;
        }
        > span{
          padding: 0 5px;
        }
      }            
    }
    .errors{
      @include un_style_ul; 
      @extend ._alert; @extend .alert_fail;
    }
    @include respond-to($media_max_mobile){
      form{
        .group_form{
          padding: 20px 10px;
          .form-item{
            width: 100%;                  
          }
        }
      }
    }
