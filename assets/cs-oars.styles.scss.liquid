  ul.navigation-links{
    $__padding_navigation: 30px;     
    width: 100%;
    text-align: center;
    font-family: $font_1;
    .sub-dropdown{
      height: $__padding_header;
    }    
    p{
      margin-bottom:  0 !important;
    }        
    > .dropdown {
      > a{        
        > p{
          position: relative;
          margin-bottom:  0 !important;
        }       
      }
    }    
    > li{       
      @include inline-block();
      margin-left: $__padding_navigation / 2;
      margin-right: $__padding_navigation / 2;
      line-height: 35px;
      &:last-child{
        margin-right: 0;
      }
      &:first-child{
        margin-left: 0;
      }
      > a{
        display: block;
        text-transform: uppercase;
        color: $color_navigation_text;
        font-size: $_h7;                                
        font-weight: $font_weight_bold1;
        position: relative;                       
        > p{
          letter-spacing: 0.075em;
          @include inline-block();
        }
        > span{
          letter-spacing: 0.075em;
        }
      }
      &.active, &:hover, &:focus{
        > a{
          color: $color_navigation_text_hover;                    
        }        
      }      
      .dropdown-menu{
        border-top: 2px solid $color_primary !important;                
      }      
      &:nth-child(2){
        > .dropdown-menu{
          //display: block !important;
        }
      }
      @include respond-to($media_tablet){
        margin-left: $__padding_navigation / 2 - 5px;
        margin-right: $__padding_navigation / 2 - 5px;
      }
    }  
    /*MEGA MENU*/
    > li{                  
      &.dropdown:not(.mega-menu){
        &:hover{
          &:before{
            width: 250px;            
          }
        }       
      }      
      &:not(.mega-menu){
        .dropdown-menu>.active>a, 
        .dropdown-menu>.active>a:focus, 
        .dropdown-menu>.active>a:hover,
        .dropdown-menu>li>a:focus, 
        .dropdown-menu>li>a:hover{
          background-color: transparent;
          color: $color_subnavigation_text_hover;
        }    
        >.dropdown-menu{  
          left: 50%;          
          @include transform(translateX(-50%));          
        }
        .dropdown-menu{          
          margin: 0px;          
          margin-top: $__padding_header;
          width: 210px;
          padding: 0px;
          min-width: 170px;                              
          @include box-shadow(0 1px 15px rgba(0,0,0,0.05));
          .sub-dropdown{
            right: 100%;
            left: auto;
            top: 0;
            bottom: auto;
            height: 100%;
            width: 10px;
          }
          li{
            padding-top: 5px;
            padding-bottom: 5px;
            padding-left: 20px;
            padding-right: 20px;
          }
          > li{ 
            padding-bottom: 0;
            padding-top: 0;            
            > a{
              color: $color_subnavigation_text;              
              border-bottom: 1px solid $color_navigation_border;
              padding: 15px ;
              font-weight: $font_weight_normal;              
              @include transition();
            }
            &:last-child{
              > a{
                border-bottom: 0;
              }
            }
            &:hover, &:focus,
            &.active {
              a{
                color: $color_subnavigation_text_hover;
              }
            }
          }
          > li{
            position: relative;
            .dropdown-menu{
              position: absolute;
              top: 0;
              right: 100%;     
              left: auto; 
              margin-top: 0;
            }
            @include respond-to($media_min_desktopsmall){
              .dropdown-menu{
                display: block;
                @include opacity(0,0);
                margin-right: 10px;
                @include transition();
              }
              &:hover, &:focus{
                > .dropdown-menu{
                  @include opacity(1,100);
                  margin-right: 0px;
                }
              }
              > li{
                &:hover, &:focus{
                  > .dropdown-menu{
                    @include opacity(1,100);
                    margin-right: 0px;
                  }
                }
              }
            }                        
          }                    
        }                 
      }      
      &.mega-menu{         
        position: static;
        .megamenu-container{ 
          $width_container_desktopsmall: 1170px;
          margin: 0;        
          padding-top: 0;
          padding-bottom: 0;          
          overflow: hidden;          
          background-repeat: no-repeat;
          background-position: right bottom;
          background-size: auto 100%;
          left: 50%;           
          border-width: 0px;
          border-top-width: 0;          
          background-color: $color_content_bg;
          color: $color_subnavigation_text;
          @include transform(translateX(-50%));
          @include box-shadow(0 2px 15px rgba(0,0,0,0.05));                    
          width: $width_container_desktopsmall;
          z-index: $z_index_slideshow + 2;
          a{
            color: $color_subnavigation_text;
            &:hover{
              color: $color_subnavigation_text_hover;
            }
          }  
          .sub-mega-menu{
            @include display-flexbox();                    
            @include flex-wrap(wrap);            
            padding-bottom: 60px;
            padding-top: 30px;      
            padding-left: 100px;
            padding-right: 100px;
            color: $color_subnavigation_text;
            .mega-links{
              .list-title{
                font-size: 16px;
                color: $color_heading;
                font-weight: $font_weight_normal;
                margin-bottom: 10px;
              }
              > ul {
                margin-bottom: 30px;
                > li{
                  line-height: 1.5em;
                  margin-bottom: 5px;
                }
              }
            }            
          }
          &.megamenu-container-1,
          &.megamenu-container-2{            
            padding-right: $width_full / 7;
            .sub-mega-menu{                                          
              padding-right: 0;
              > li{
                width: $width_full / 3;
              }
            }
          }          
          &.megamenu-container-3{
            .sub-mega-menu{
              > li{
                width: $width_full / 4;
              }
            }
          }
          @include respond-to($media_tablet){
            $width_container_tablet: 970px;
            width: $width_container_tablet;
          }
        }                               
      }      
    }        
  } 
