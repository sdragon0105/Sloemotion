  .faqs-content{    
    $bg_navcolor: #fff;  
    $bg_navcolor_hover: #fff;      
    $color_text_hover: $color_primary;
    $color_icon: $color_body;
    $color_icon_hover: $color_primary;
    $_padding-nav: 8px; 
    width: 100%;
    float: left;
    margin-bottom: $distance_section_subpage;    
    @include respond-to($media_max_mobilemedium){
      margin-bottom: 0;
    }
    .nav-tabs{
      border-bottom: 0;
      >li{
        width: 100%;        
        border-bottom: 1px solid $color_line;
        margin-bottom: 0;                
        > a{          
          font-size: 14px;
          background-color: $bg_navcolor;
          color: $color_body;
          margin-right: 0;
          border-radius: 0;
          padding: $_padding-nav 30px;
          position: relative;
          .fa{
            position: absolute;
            top: $_padding-nav;
            left: 0;
            height: 20px;
            line-height: 20px;
          }
        }    
        &:last-child{
          border-bottom: 0;
        }
        &:hover{
          &:before{
            color: $color_icon_hover;
          }
        }        
      }
      @media(max-width: 991px){
        margin-bottom: 30px;
      }
    }
    .nav-tabs>li >a,
    .nav-tabs>li.active>a, 
    .nav-tabs>li.active>a:focus, 
    .nav-tabs>li.active>a:hover{
      border: 0;
    }
    .nav-tabs>li:hover >a,
    .nav-tabs>li.active>a, 
    .nav-tabs>li.active>a:focus, 
    .nav-tabs>li.active>a:hover{
      background-color: $bg_navcolor_hover;
      color: $color_text_hover !important;
    }
    .panel-title{
      color: $color_heading;
    }
    .faqs-inner{ @include padding-container();
      > .col-md-7{
        border-left: 1px solid $color_line;
        @media(max-width: 991px){
          border-left: 0;
        }
      }
      .tab-content{     
        .panel-group .panel+.panel {
          margin-top: 10px;
        }
        .panel-group .panel {                
          border-radius: 0;
          box-shadow: none;
        } 
        .panel-default>.panel-heading+.panel-collapse>.panel-body {
          border-top-color: transparent;
        }
        .panel-default>.panel-heading{
          background-color: transparent;
        }
        .panel-body {
          padding: 0 20px 25px;
          font-size: 14px;
        }
        .faq-heading{
          padding: 0 15px;
          border-top-left-radius: 0;
          border-top-right-radius: 0;
          border-top: 0;
          margin-bottom: 13px;
          position: relative;
          font-style: normal;                  
          font-size: 22px;   
          color: $color_heading;          
          font-weight: $font_weight_bold;
          @media (max-width: 991px){
            padding: 0;
          }
        }  
        .faq-content{
          padding-left: 15px;
          line-height: 1.95em;
          @media (max-width: 991px){
            padding-left: 0px;
          }
        }   
        .clearfix{
          display: block;
          margin-bottom: 29px;          
        }          
      } 
      @media(max-width: 767px){
        margin-left: 0px;
        margin-right: 0px;
      }
    }  
    .faqs-title{
      font-size: $_h4; 
      font-weight: $font_weight_bold; 
      text-transform: uppercase;
      color: $color_heading;          
      margin-bottom: $distance_section_subpage;
      margin-top: 60px;
