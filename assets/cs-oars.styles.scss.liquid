@mixin newletter_form_style($__height: 40px){  
  form{ 
    width: 300px;
    max-width: 100%;
    position: relative; 
    height: $__height;     
    border-radius: 0;
    border: 1px solid hsla(hue($color_footer_border), saturation($color_footer_border), lightness($color_footer_border), 1);
    background-color: $color_footer_bg;    
    input{ 
      padding: 0 20px 0 20px; 
      width: 100%; 
      height: 100%; 
      color: $color_footer_text;
      border-width: 0;  
      line-height: $__height;
      background-color: transparent; 
      font-size: $_h7; 
      font-weight: $font_weight_thin;
      @include box-shadow(none);
      @include placeholder('&'){
        color: $color_footer_text;
        font-weight: $font_weight_thin;
      }
      &:focus{
        @include box-shadow(none);
        @include placeholder('&'){
          color: transparent;
        }
      }
    }     
    button{ 
      position: absolute; top: 0; right: 0; bottom: 0; padding: 0;             
      width: $__height*2 + 20px; 
      background-color: $color_sub;      
      border-width: 0;
      color: #fff;      
      font-size: 14px;
      padding-left: 15px;
      padding-right: 15px;
      &:hover,
      &:focus{ 
        outline: none; 
        background-color: #000;        
        .cs-icon{
          color: $color_sub;
        }
      }
      * { 
        font-size: 26px; 
        color: $color_footer_title_text; 
        line-height: $__height;
      }
    }   
  }  
}
